o:Sass::Tree::RootNode	:@template"K"// Type
$size: 12px
$lead: 18px

// Metrics
$columns: 12
$column: 85px
$gutter: 10px
$margin: 10px
$top: $margin
$bottom: $margin

// Colors
$primary: #ec1f00
$secondary: #006196
$tertiary: #f3a015
$text: #b2b2b2
$page: #000
$body: #5c5d5c

// Fonts
$font: Verdana, sans-serif
$headline: NeutrafaceText, Gill Sans, Verdana, sans-serif
$headline-weight: 700
$headline-style: normal
$subhead: NeutrafaceText, Gill Sans, Verdana, sans-serif

// Image/page ratios
=ratios
  +ratio(6, 4, $columns)
  +ratio(4, 6, $columns)

=headline
  font-family: $headline
  font-weight: $headline-weight
  font-style: $headline-style


// Column widths
.column
  +width(3)
  +type($size, $lead)

// Grid styles

.grid
  +box-shadow(none !important)
  margin-left: 0 !important
  opacity: 1 !important
  &#previousPage, &#nextPage
    .column, .container, .runningtitle
      opacity: .5 !important
  &.page-1
    margin-left: $margin*2 !important
    +corner-radius(top, left, 10px)
    +corner-radius(bottom, left, 10px)
  &.last-page
    +corner-radius(top, right, 10px)
    +corner-radius(bottom, right, 10px)
  .runningtitle
    z-index: 1
    display: block
    position: relative
    top: $margin
    width: 135px
    margin-top: 20px
    border: 1px solid $secondary
    border-left-width: 37px
    background-color: #fff
    color: $page
    +caps
    font-family: $subhead
    font-weight: bold
    +solid(10px)
    padding: 4px 4px
  .box-story
    +border-radius($margin)
    padding: $margin
    background: $tertiary
    color: #fff
    font-family: $subhead
    font-weight: bold
    +type(14px, 20px)
  .fullbleed
    top: $margin !important
    right: $margin !important
    bottom: $margin !important
    left: $margin !important
  .footer.keeptogether
    border-top: 2px solid #fff
    margin-top: 10px
    padding-top: 10px
    img
      float: left
      padding-right: 12px
    p
      +headline
      +solid(14px)
      font-weight: normal
      +caps(.2em)
      padding: 6px 0
    a
      padding-top: 6px
      font-family: $subhead
      font-weight: bold
      color: $text
      display: block
      clear: both
  .opener
    img
      display: block
    .intro
      position: absolute
      bottom: $gutter
      right: $gutter
      width: ($column*3 + $gutter)
      padding: $gutter
      padding-right: 0
      background-color: #fff
      h3
        margin-right: $gutter
        background-color: #000
        color: #fff
        padding: 3px 4px
        font-family: $subhead
        +type(15px, 18px)
      p
        color: #000
        margin-right: $gutter
        font-family: $subhead
        +type(15px, 18px)
  .w3col
    .intro
      display: none
  div.pullquote
    text-align: center
    padding: $lead*2 0
    height: $lead*2
    +transform(rotate(0) scale(0))
    +transition(all .6s ease-out)
    p.pullquote
      display: inline
      background-color: #009ee1
      color: #fff
      font-family: $subhead
      font-weight: bold
      +solid($size*3)
      padding: 0 12px
  &#currentPage
    div.pullquote
      +transform(rotate(-4deg) scale(1))

// Typographic styles
h1, h2, h3, h4, h5, h6, .title
  font-family: $subhead
  font-weight: bold
  color: #fff

h1
  +headline
  +solid($lead*2)
  +text-shadow(2px 2px 4px #000)
  background-repeat: repeat-x

// Grid styles

=columns($cols)
  @for $i from 1 through $cols
    .w#{$i}col.category.grid
      width: (($column * $i + $gutter * ($i - 1))) - 55px

+columns($columns)


.cover.grid
  padding: $margin
  +border-radius($margin)
  .img
    top: $margin
    left: $margin
  .os-ipad.orientation-vertical &.r6x4
    +over9000

.category.grid
  padding: 30px 20px 20px 20px
  .column, .container
    top: 30px
    bottom: 20px
  .runningtitle
    margin-top: 0
    top: 0
    border-color: $primary
  .intro
    display: none
  h1
    height: $lead*9
    width: 200px
    padding: 20px 0 0 0
    font-weight: bold
    +solid($lead*3)
    a
      color: $primary
      font-weight: bold
  h2
    background: #484848
    height: 165px
    margin-bottom: $lead
    width: 200px
    overflow: hidden
    +type(16px, 19px)
    +text-shadow(0 0 3px #000)
    padding: 10px 10px 0 10px
    a
      color: #fff
  img
    width: 220px
    height: 147px
    margin: -10px -10px 0 -10px

.article.grid
  .title
    overflow: visible !important
    h1
      background-image: url('../resources/title-bkg.png')
      padding: 0 6px
    &.w3col h1
      margin-top: -42px
    &.w6col h1
      margin-top: -32px
    &.w9col h1
      margin-top: -40px
  &.flexable
    &.no-page-1
      .runningtitle
        margin-top: 0
      .container, .column
        top: $lead*2
  &.mobile.horizontal
    &.page-1
      overflow: visible !important
      .title
        +headline
        position: absolute
        z-index: 1
        bottom: $margin
        left: $margin
        right: $margin
        background-image: url('../resources/title-bkg.png')
        +solid(20px)
        padding: 2px
        height: 26px
        +text-shadow(1px 2px 4px #000)
    &.page-2
      .runningtitle
        margin-top: 0
      .title
        display: none
        h1
          margin-top: $lead*1.5
  &.mobile.vertical
    margin-left: 20px !important
    &.page-1
      .runningtitle
        margin-top: 0
        top: 0
      overflow: visible !important
      .title
        overflow: visible !important
        h1
          background-image: url('../resources/title-bkg.png')
          +solid(20px)
          +text-shadow(1px 2px 4px #000)
          margin-top: -28px
    &.page-2
  &.ipad.horizontal
    &.page-1
      .title
        overflow: visible !important
        position: absolute
        display: block
        z-index: 1
        bottom: $margin - 4px
        left: $margin
        right: $margin
        background-image: url('../resources/title-bkg.png')
        padding: 0 6px
        +headline
        +solid($lead*2)
        +text-shadow(2px 2px 4px #000)
        background-repeat: repeat-x
    &.page-2
      .runningtitle
        margin-top: 0
      .title
        padding-top: $lead*2
  &.ipad.vertical
    &.page-1
      .title
        overflow: visible !important

.big.grid
  .runningtitle
    border-color: #cc071e
  h1
    color: #009fe1
  .box-story
    background-color: #009fe1
    +border-radius(0)
  &.ipad
    .intro
      text-align: left
    .title
      overflow: visible !important
      h1
        background-image: url('../resources/title-bkg.png')
        +solid(36px)
        +text-shadow(1px 2px 4px #000)
        margin-top: 0
        padding-bottom: $lead
  &.mobile
    .intro
      display: none

.photos.grid
  &.ipad.horizontal
    .column, .container
      top: $lead*3
  .title
    overflow: visible !important
    h1
      background-image: url('../resources/title-bkg.png')
      padding: 0 6px
    &.w4col h1
      margin-top: -49px
    &.w6col h1
      margin-top: -33px
    &.w8col h1
      margin-top: -49px
    &.w12col h1
      margin-top: -48px
    &.textpage h1
      margin-top: 0
  .column
    +width(4)
  .runningtitle
    margin-top: 0
  .box-story
    background-color: #d6b377
    color: #000
  .caption
    position: absolute
    width: ($column*3 + $gutter)
    padding: $gutter
    padding-right: 0
    background-image: url('../resources/transp-caption-bkg.png')
    +box-shadow(0 0 5px #000)
    font-weight: bold
    color: #fff
    text-align: left
  &.ipad.horizontal
    .title.running
      overflow: visible !important
      position: absolute
      display: block
      z-index: 1
      bottom: $margin - 4px
      left: $margin
      right: $margin
      background-image: url('../resources/title-bkg.png')
      padding: 0 6px
      +headline
      +solid($lead*2)
      +text-shadow(2px 2px 4px #000)
      background-repeat: repeat-x

.lightbox
  .pan
    overflow: hidden
    img
      +transition(all 10s ease)
      +transform(translate(-950px,1300px) scale(5))
      &:hover
        +transform(translate(0px,0px) scale(1))

.puller
  position: absolute
  top: 0
  bottom: 0
  right: 0
  width: 34px
  background: url(../resources/puller.png)
  background-position: center
  background-repeat: no-repeat
  .last-page &
    display: none
//  #previousPage &
//    margin-right: 10px

.ad.grid
  +corner-radius(top, right, 10px)
  +corner-radius(bottom, right, 10px)
  padding: 10px
  margin-left: 20px !important
  +border-radius(10px)
  .img
    top: 10px
    left: 10px

.category.grid
  h1
    height: 144px
  h2
    margin-bottom: 0px
  &.ipad.vertical
    h1
      height: 162px
    h2
      margin-bottom: 18px

// MOBILE STYLES

.grid.mobile
  .container, .column
    top: 0
  &.horizontal
    max-height: 300px
  &.vertical
    min-height: 300px


.no-mobile
  .grid.ipad
    +over9000

.os-ipad
  .grid.flexable
